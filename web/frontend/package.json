    "react-dom": "^18.3.1",
    "react-use-websocket": "^4.11.1",
    "recharts": "^2.15.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.15.0",
